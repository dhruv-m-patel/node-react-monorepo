{
  "compilerOptions": {
    "target": "es2020",
    "module": "CommonJS",
    "jsx": "react",
    "lib": ["dom", "es2020"],
    "strict": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "typeRoots": ["node_modules/@types"]
  }
}
