{
  "name": "starter-app",
  "version": "1.0.0",
  "description": "A starter app for frontend projects with universal react, material-ui and redux support",
  "main": "build/server/index.js",
  "license": "MIT",
  "scripts": {
    "build": "tsc && webpack",
    "clean": "rimraf build ",
    "start:watch": "yarn run clean && yarn run build && nodemon",
    "lint:ts": "eslint src/**/*.ts && eslint src/**/*.tsx",
    "lint:css": "stylelint \"./src/**/*.tsx\"",
    "lint": "yarn run lint:ts && yarn run lint:css",
    "lint:fix": "eslint --fix .",
    "prettier:verify": "prettier --check .",
    "test": "jest --runInBand --passWithNoTests",
    "test:ci": "jest --silent --passWithNoTests --ci --coverage --coverageReporters=text --coverageReporters=text-summary",
    "test:changed": "jest --runInBand --onlyChanged",
    "storybook": "start-storybook -p 6000",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@dhruv-m-patel/react-components": "^1.0.0",
    "@dhruv-m-patel/web-app": "^1.0.0",
    "@loadable/component": "^5.14.1",
    "@loadable/server": "^5.14.0",
    "@material-ui/core": "^4.11.2",
    "@material-ui/icons": "^4.11.2",
    "bluebird": "^3.7.2",
    "clsx": "^1.1.1",
    "core-js": "^3.8.1",
    "dotenv": "^10.0.0",
    "fast-deep-equal": "^3.1.3",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-helmet-async": "^1.0.7",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "redux-api-middleware": "^3.2.1",
    "store": "^2.0.12",
    "webpack": "^4.44.2"
  },
  "devDependencies": {
    "@storybook/addon-a11y": "^6.1.11",
    "@storybook/addon-actions": "^6.1.11",
    "@storybook/addon-essentials": "^6.1.11",
    "@storybook/addon-links": "^6.1.11",
    "@storybook/react": "^6.1.11",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^11.2.2",
    "@types/express": "^4.17.12",
    "@types/jest": "^26.0.23",
    "@types/loadable__component": "^5.13.1",
    "@types/loadable__server": "^5.12.3",
    "@types/node": "^15.12.5",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-redux": "^7.1.14",
    "@types/react-router-dom": "^5.1.6",
    "@types/redux-api-middleware": "^3.2.2",
    "@types/store": "^2.0.2",
    "@types/testing-library__jest-dom": "^5.14.1",
    "eslint-plugin-react": "7.24.0",
    "jest": "27.0.5",
    "jest-junit": "^12.2.0",
    "nodemon": "^2.0.6",
    "react-test-renderer": "^16.14.0",
    "rimraf": "^3.0.2",
    "stylelint": "^13.8.0",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0",
    "ts-jest": "^27.0.3",
    "ts-loader": "^8.0.12",
    "ts-node": "^10.0.0",
    "typescript": "4.3.4",
    "webpack-cli": "^3.3.12"
  }
}
