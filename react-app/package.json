{
  "name": "react-app",
  "version": "1.0.0",
  "description": "A universal react app with styled-components and redux support",
  "main": "build/server/index.js",
  "private": true,
  "scripts": {
    "build:server": "babel ./src --out-dir build",
    "build:client": "BABEL_ENV=webpack webpack",
    "build": "npm run build:server && npm run build:client",
    "clean": "rimraf build && rimraf build-static",
    "start": "NODE_ENV=production node ./build/server/index.js",
    "start-dev": "NODE_ENV=development babel-node src/server",
    "start:watch": "npm run clean && npm run build && nodemon",
    "lint:js": "eslint src/**/*.js && eslint src/**/*.jsx",
    "lint:css": "stylelint \"./src/**/*.jsx\"",
    "lint": "npm run lint:js && npm run lint:css",
    "pretest": "jest --clearCache",
    "prettier:format": "prettier --write .",
    "prettier:verify": "prettier --check .",
    "test": "jest --runInBand",
    "test:ci": "jest --silent --ci --coverage --coverageReporters=text --coverageReporters=text-summary",
    "storybook": "start-storybook -p 3001",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@babel/runtime": "^7.11.2",
    "@dhruv-m-patel/express-app": "^1.0.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.30",
    "@fortawesome/free-brands-svg-icons": "^5.14.0",
    "@fortawesome/free-regular-svg-icons": "^5.14.0",
    "@fortawesome/free-solid-svg-icons": "^5.14.0",
    "@fortawesome/react-fontawesome": "^0.1.11",
    "@loadable/component": "^5.13.2",
    "@loadable/server": "^5.13.2",
    "bluebird": "^3.7.2",
    "confit": "^3.0.0",
    "core-js": "^3.6.5",
    "dotenv": "^8.2.0",
    "eslint-config-prettier": "^8.3.0",
    "express": "^4.17.1",
    "express-enrouten": "^1.3.0",
    "fast-deep-equal": "^3.1.3",
    "fetch-everywhere": "^1.0.5",
    "morgan": "^1.10.0",
    "react": "^16.13.1",
    "react-bootstrap": "^1.3.0",
    "react-dom": "^16.13.1",
    "react-helmet-async": "^1.0.7",
    "react-redux": "^7.2.1",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "redux-api-middleware": "^3.2.1",
    "shortstop-handlers": "^1.0.1",
    "shortstop-regex": "0.0.1",
    "store": "^2.0.12",
    "styled-components": "^5.2.0",
    "webpack": "^4.44.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/node": "^7.10.5",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.11.0",
    "@babel/plugin-proposal-optional-chaining": "^7.11.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-syntax-optional-chaining": "^7.8.3",
    "@babel/plugin-transform-react-inline-elements": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "@loadable/babel-plugin": "^5.13.2",
    "@loadable/webpack-plugin": "^5.13.0",
    "@storybook/addon-a11y": "^6.0.21",
    "@storybook/addon-actions": "^6.0.21",
    "@storybook/addon-essentials": "^6.0.21",
    "@storybook/addon-links": "^6.0.21",
    "@storybook/react": "^6.0.21",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.0.4",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.3.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-macros": "^2.8.0",
    "babel-plugin-styled-components": "^1.11.1",
    "eslint-plugin-react": "^7.20.6",
    "jest": "^26.4.2",
    "react-is": "^16.13.1",
    "react-test-renderer": "^16.13.1",
    "rimraf": "^3.0.2",
    "stylelint": "^13.7.1",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0",
    "terser-webpack-plugin": "^4.2.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-manifest-plugin": "^2.2.0",
    "webpack-node-externals": "^2.5.2"
  }
}
