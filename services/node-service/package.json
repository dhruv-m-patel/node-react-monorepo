{
  "name": "node-service",
  "version": "1.0.0",
  "description": "Backend built with node, express and typescript",
  "main": "build/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node build/index.js",
    "start-dev": "ts-node src/index.ts",
    "test": "jest --runInBand",
    "test:ci": "jest --silent --ci --coverage --coverageReporters=text --coverageReporters=text-summary",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-openapi-validator": "^4.12.10",
    "js-yaml": "^4.1.0"
  },
  "devDependencies": {
    "@types/compression": "^1.7.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/js-yaml": "^4.0.0",
    "@types/supertest": "^2.0.11",
    "jest": "26.6.0",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.4",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4"
  }
}
