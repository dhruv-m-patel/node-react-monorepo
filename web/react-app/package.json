{
  "name": "react-app",
  "version": "1.0.0",
  "description": "A universal react app with material-ui and redux support",
  "main": "build/server/index.js",
  "scripts": {
    "build": "tsc && webpack",
    "clean": "rimraf build ",
    "start": "node ./build/server/index.js",
    "start-dev": "ts-node --prefer-ts-exts src/server/index.ts",
    "start:watch": "npm run clean && npm run build && nodemon",
    "lint:ts": "eslint src/**/*.ts && eslint src/**/*.tsx",
    "lint:css": "stylelint \"./src/**/*.tsx\"",
    "lint": "npm run lint:ts && npm run lint:css",
    "lint:fix": "eslint --fix .",
    "prettier:verify": "prettier --check .",
    "pretest": "jest --clearCache",
    "test": "jest --runInBand",
    "test:ci": "jest --coverage",
    "storybook": "start-storybook -p 3001",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@loadable/component": "^5.14.1",
    "@loadable/server": "^5.14.0",
    "@material-ui/core": "^4.11.2",
    "@material-ui/icons": "^4.11.2",
    "bluebird": "^3.7.2",
    "body-parser": "^1.19.0",
    "clsx": "^1.1.1",
    "confit": "^3.0.0",
    "cookie-parser": "^1.4.5",
    "core-js": "^3.8.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-enrouten": "^1.3.0",
    "fast-deep-equal": "^3.1.3",
    "fetch-everywhere": "^1.0.5",
    "meddleware": "^3.0.4",
    "morgan": "^1.10.0",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-helmet-async": "^1.0.7",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "redux-api-middleware": "^3.2.1",
    "shortstop-handlers": "^1.0.1",
    "shortstop-regex": "0.0.1",
    "store": "^2.0.12",
    "webpack": "^4.44.2"
  },
  "devDependencies": {
    "@loadable/webpack-plugin": "^5.14.0",
    "@storybook/addon-a11y": "^6.1.11",
    "@storybook/addon-actions": "^6.1.11",
    "@storybook/addon-essentials": "^6.1.11",
    "@storybook/addon-links": "^6.1.11",
    "@storybook/react": "^6.1.11",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^11.2.2",
    "@types/confit": "^2.0.3",
    "@types/express": "^4.17.9",
    "@types/jest": "^26.0.19",
    "@types/loadable__component": "^5.13.1",
    "@types/loadable__server": "^5.12.3",
    "@types/node": "^14.14.16",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-redux": "^7.1.14",
    "@types/react-router-dom": "^5.1.6",
    "@types/redux-api-middleware": "^3.2.2",
    "@types/store": "^2.0.2",
    "@typescript-eslint/eslint-plugin": "^4.21.0",
    "@typescript-eslint/parser": "^4.21.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.3.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-standard": "^5.0.0",
    "jest": "26.6.3",
    "nodemon": "^2.0.6",
    "react-test-renderer": "^16.14.0",
    "rimraf": "^3.0.2",
    "stylelint": "^13.8.0",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0",
    "terser-webpack-plugin": "^4.2.3",
    "ts-jest": "^26.4.4",
    "ts-loader": "^8.0.12",
    "ts-node": "^9.1.1",
    "typescript": "4.1.3",
    "webpack-cli": "^3.3.12",
    "webpack-dev-middleware": "^3.7.3",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-manifest-plugin": "^2.2.0",
    "webpack-node-externals": "^2.5.2"
  }
}
